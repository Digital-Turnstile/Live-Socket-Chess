{"ast":null,"code":"export default {\n  login: function login(user) {\n    console.log(user);\n    if (!user.username) return fetch('http://localhost:5000/api/login', {\n      method: \"post\",\n      body: JSON.stringify(user),\n      headers: {\n        'Content-Type': 'application/json'\n      }\n    }).then(function (res) {\n      if (res.status !== 401) return res.json().then(function (data) {\n        return data;\n      });else return {\n        isAuthenticated: false,\n        user: {\n          username: \"\",\n          plan: \"\",\n          token: ''\n        }\n      };\n    });\n  },\n  register: function register(user) {\n    console.log(user);\n    return fetch('http://localhost:5000/api/register', {\n      method: \"post\",\n      body: JSON.stringify(user),\n      headers: {\n        'Content-Type': 'application/json'\n      }\n    }).then(function (res) {\n      return res.json();\n    }).then(function (data) {\n      return data;\n    });\n  },\n  logout: function logout() {\n    return fetch('http://localhost:5000/api/logout').then(function (res) {\n      return res.json();\n    }).then(function (data) {\n      return data;\n    });\n  },\n  authenticate: function authenticate(user) {\n    return fetch('http://localhost:5000/api/authenticated', {\n      headers: {\n        'Authorization': 'Bearer ' + user.token\n      }\n    }).then(function (res) {\n      if (res.status !== 401) return res.json().then(function (data) {\n        return data;\n      });else return {\n        isAuthenticated: false,\n        user: {\n          username: '',\n          plan: '',\n          token: ''\n        }\n      };\n    });\n  }\n};","map":{"version":3,"sources":["/home/caesar/Projects/Workout-SaaS/Client/src/Services/AuthService.js"],"names":["login","user","console","log","username","fetch","method","body","JSON","stringify","headers","then","res","status","json","data","isAuthenticated","plan","token","register","logout","authenticate"],"mappings":"AAAC,eA